# Assigning elements to different lists

list1 = [1,2,3,4]
list2 = [5,6,7,8]

list1[4] = 6
list2[4] = 9

list1 = list1 + list2

print(list1)

# Accessing different elements of a tuple

tup = (1,2,3,4)

a = tup[2]
b = tup[1:3]

print(a)
print(b)

# Deleting different dictionary elements

dict = {'a'=[2,3,4], 'b'=4, 'c'=5}

del dict['b']
print(dict)

dict.pop('c')
print(dict)
